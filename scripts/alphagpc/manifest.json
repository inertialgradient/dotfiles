{
  "manifest_version": 3,
  "name": "alphagpc",
  "version": "1.0",
  "description": "alphagpc",
  "content_scripts": [
    {
      "matches": ["https://www.coursera.org/learn/*"],
      "js": [
        "coursera-content.js",
        "coursera-quizzes.js",
        "coursera-assignments.js",
        "coursera-observer.js"
      ]
    },
    {
      "matches": ["https://canvas.liberty.edu/courses/*/modules"],
      "js": ["canvas-parser.js"]
    },
    {
      "matches": ["https://www.gradescope.com/courses/*/assignments/*/submissions/new"],
      "js": ["gradescope-parser.js"]
    }
  ],
  "permissions": []
}
