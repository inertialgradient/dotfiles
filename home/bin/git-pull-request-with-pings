#!/usr/bin/env bash

# Submit a pull request with some suggested pings
#
# Usage:
#
# $ git pull-request-with-pings

# areas_of_responsiblity :enterprise_tools, :dotcom

set -e

message_template="

**issue**:

**changes made**

**testing notes**:

**refs**:

**cc**:
$(git pings)
"

echo "$message_template" | hub pull-request -F -
