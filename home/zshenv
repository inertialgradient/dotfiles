#-------------------------------------------------------------
# N / NODE
#-------------------------------------------------------------
export N_PREFIX="$HOME/.node"

#-------------------------------------------------------------
# GO
#-------------------------------------------------------------
export GOPATH=$HOME/.go
export GOROOT=/usr/local/opt/go/libexec

#-------------------------------------------------------------
# ELM
#-------------------------------------------------------------
export ELM_HOME=$HOME/.elm

#-------------------------------------------------------------
# EXENV
#-------------------------------------------------------------
if which exenv > /dev/null; then
  eval "$(exenv init -)";
fi
