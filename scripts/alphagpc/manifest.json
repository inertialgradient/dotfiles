{
  "manifest_version": 3,
  "name": "alphagpc",
  "version": "1.0",
  "description": "alphagpc",
  "content_scripts": [
    {
      "matches": ["https://www.coursera.org/learn/*/lecture/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://www.coursera.org/learn/**/*/attempt"],
      "js": ["quizzes.js"]
    },
    {
      "matches": ["https://www.coursera.org/learn/**/*/view-submission"],
      "js": ["quizzes.js"]
    },
    {
      "matches": ["https://www.coursera.org/learn/*/home/assignments"],
      "js": ["assignments.js"]
    },
    {
      "matches": ["https://canvas.liberty.edu/courses/*/modules"],
      "js": ["parser-canvas.js"]
    }
  ],
  "permissions": []
}
